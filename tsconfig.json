{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "moduleResolution": "node",
    "declaration": true,
    "noImplicitAny": true,
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "noEmitOnError": true,
    "experimentalDecorators": true,
    "outDir": "built/",
    "typeRoots": [
      "node_modules/@types"
    ],
    "types": [
      "chalk", "commander", "nedb", "node"
    ],
    "lib": ["es6"]
  },
  "exclude":[
    "node_modules"
  ]
}
